<%= form_with(url: search_musics_path, local: true, method: :get, class: "search-form") do |form| %>
  <%= form.text_field :keyword, placeholder: "Please search for Artist Name, Song Name, Album Name~♪", class: "search-input" %>
  <%= form.submit "🔍", class: "search-btn" %>
<% end %>
<div class="contents row">
  <%= music_lists(@musics) %>

</div>